    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.2",
    "solhint": "^3.3.2",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.13",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^3.0.0",
    "typescript": "^4.1.3"
